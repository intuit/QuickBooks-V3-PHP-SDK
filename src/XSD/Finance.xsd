						</xs:annotation>
					</xs:element>
					<xs:element name="EmployerId" type="xs:string"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Product: ALL
								Description: Employer identifier (EID).
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FiscalYearStartMonth" type="MonthEnum"
