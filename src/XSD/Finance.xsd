	<xs:simpleType name="SpecialTaxTypeEnum">
		<xs:annotation>
			<xs:documentation>
				Product: QBO
				Description: Enumeration of
				SpecialTaxType
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
